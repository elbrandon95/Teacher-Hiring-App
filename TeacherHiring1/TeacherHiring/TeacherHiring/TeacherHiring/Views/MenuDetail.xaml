<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TeacherHiring.Views.MenuDetail"
             Title="Detail">
    <StackLayout Padding="10">
        <Label x:Name="lblBienvenida" YAlign="Center" XAlign="Center" Text="{Binding myProperty}"/>
        <Label x:Name="lblUserName" YAlign="Center" XAlign="Center" Text="{Binding userName}"/>
    </StackLayout>
</ContentPage>